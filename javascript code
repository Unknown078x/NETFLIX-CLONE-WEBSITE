function hideseek(){
    let click1 = document.querySelector('.heed1');
let hide1 = document.querySelector('.middle1');
let hide2 = document.querySelector('.middle2')
let cross1 = document.querySelector('#plus1');
var display = 0
click1.addEventListener('click', () => {
    if (display == 0) {
        hide1.style.height = '38vh'
        cross1.style.rotate = '45deg'
        cross1.style.transition = 'rotate 0.2s'
        display = 1
        hide2.style.height = '0'
        cross2.style.rotate = '0deg'
        hide3.style.height = '0'
        cross3.style.rotate = '0deg'
        hide4.style.height = '0'
        cross4.style.rotate = '0deg'
        hide5.style.height = '0'
        cross5.style.rotate = '0deg'
        hide6.style.height = '0'
        cross6.style.rotate = '0deg'
    }
    else {
        hide1.style.height = '0'
        cross1.style.rotate = '0deg'
        display = 0
    }

});
let click2 = document.querySelector('.heed2');
let cross2 = document.querySelector('#plus2');
click2.addEventListener('click', () => {
    if (display == 0) {
        hide2.style.height = '18.5vh'
        cross2.style.rotate = '45deg'
        cross2.style.transition = 'rotate 0.2s'
        display = 1
        hide1.style.height = '0'
        cross1.style.rotate = '0deg'
        hide3.style.height = '0'
        cross3.style.rotate = '0deg'
        hide4.style.height = '0'
        cross4.style.rotate = '0deg'
        hide5.style.height = '0'
        cross5.style.rotate = '0deg'
        hide6.style.height = '0'
        cross6.style.rotate = '0deg'
    }
    else {
        hide2.style.height = '0'
        cross2.style.rotate = '0deg'
        display = 0
    }

});
